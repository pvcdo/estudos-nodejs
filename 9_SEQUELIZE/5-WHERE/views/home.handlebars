<h1>Usuários cadastrados</h1>
<div class="container">
  {{#each users}}
    <div class="item">
      <h2>{{this.name}}</h2>
      <p>{{this.occupation}}</p>
      <div class="actions">
        <a href="/users/{{this.id}}">Ver detalhes</a> <!-- this.id é criado automaticamente pelo sequelize quando do registro do dado no banco --> 
      </div>
    </div>
  {{/each}}
</div>